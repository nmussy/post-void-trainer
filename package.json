{
  "name": "post-void-trainer",
  "version": "0.1.1",
  "description": "CLI trainer for YCJY's Post Void",
  "main": "index.js",
  "repository": "https://github.com/nmussy/post-void-trainer.git",
  "author": "Jimmy Gaussen <jimmy.gaussen@gmail.com>",
  "license": "MIT",
  "private": true,
  "bin": "build/index.js",
  "scripts": {
    "release": "yarn build && mkdir release && pkg --target win . --output release/post-void-trainer.exe",
    "build": "yarn clean && mkdir build && babel index.js --out-file build/index.js",
    "clean": "rimraf build release",
    "start": "babel-node index.js",
    "watch": "nodemon -I --watch index.js --exec \"yarn babel-node index.js\""
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/eslint-parser": "^7.11.3",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "nodemon": "^2.0.4",
    "pkg": "^4.4.9",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ws": "^7.3.1"
  },
  "dependencies": {
    "core-js": "3",
    "inquirer": "^7.3.3",
    "memoryjs": "^3.2.3"
  }
}
